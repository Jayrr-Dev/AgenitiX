{
	"name": "agentix-2",
	"version": "0.12.0",
	"private": true,
	"scripts": {
		"dev": "pnpm run dev:setup && pnpm run dev:parallel",
		"dev:setup": "npx convex dev --until-success",
		"dev:parallel": "npm-run-all --parallel dev:convex dev:frontend",
		"dev:convex": "npx convex dev",
		"dev:frontend": "next dev --turbo",
		"predev": "pnpm run dev:setup",
		"gen-registry": "node scripts/gen-registry.js",
		"build": "pnpm run build:convex:conditional && pnpm run build:frontend && pnpm sentry:sourcemaps:conditional",
		"build:convex": "npx convex deploy",
		"build:convex:conditional": "node -e \"if (process.env.VERCEL && process.env.CONVEX_DEPLOY_KEY && process.env.VERCEL_ENV === 'production') { require('child_process').execSync('npx convex deploy', { stdio: 'inherit' }); } else { console.log('Skipping Convex deployment - not on Vercel production or missing requirements'); }\"",
		"build:frontend": "pnpm run generate:handle-types && next build --turbopack",
		"prebuild": "pnpm generate:tokens",
		"convex:dev": "npx convex dev",
		"convex:deploy": "npx convex deploy",
		"convex:dashboard": "npx convex dashboard",
		"convex:logs": "npx convex logs",
		"convex:auth": "npx convex auth",
		"convex:import": "npx convex import",
		"convex:export": "npx convex export",
		"convex:run": "npx convex run",
		"convex:codegen": "npx convex codegen",
		"convex:reinit": "npx convex reinit",
		"convex:functions:list": "npx convex functions list",
		"convex:functions:clear": "npx convex functions clear",
		"db:seed": "npx convex run --prod seedDatabase",
		"db:clear": "npx convex run --prod clearDatabase",
		"db:backup": "npx convex export --output ./backups/backup-$(date +%Y-%m-%d-%H-%M-%S).jsonl",
		"db:restore": "npx convex import",
		"dev:setup-data": "node scripts/setup-dev-data.js",
		"test:convex": "npx convex run --prod testFunctions",
		"prod:deploy": "npx convex deploy --prod",
		"prod:logs": "npx convex logs --prod",
		"start": "next start",
		"lint": "biome lint .",
		"lint:fix": "biome check --write .",
		"lint:ci": "biome ci .",
		"format": "biome format --write .",
		"plop": "plop",
		"new:node": "plop node",
		"new:category": "plop category",
		"generate:handle-types": "node scripts/gen-handle-types.js",
		"generate:tokens": "npx ts-node --project tsconfig.node.json scripts/gen-tokens.ts",
		"generate:docs": "npx ts-node --project tsconfig.node.json scripts/gen-docs-tokens.ts",
		"generate:node-docs": "npx ts-node --project tsconfig.node.json scripts/generate-node-docs.ts",
		"generate:nodes-overview": "npx ts-node --project tsconfig.node.json scripts/generate-nodes-overview.ts",
		"generate:handle-docs": "npx ts-node --project tsconfig.node.json scripts/generate-handle-docs.ts",
		"generate:ui-overview": "npx ts-node --project tsconfig.node.json scripts/generate-ui-overview.ts",
		"regenerate:ui-docs": "npx ts-node --project tsconfig.node.json scripts/regenerate-ui-docs.ts",
		"generate:infrastructure-overview": "npx ts-node --project tsconfig.node.json scripts/generate-infrastructure-overview.ts",
		"generate:theming-overview": "npx ts-node --project tsconfig.node.json scripts/generate-theming-overview.ts",
		"generate:nodespec-docs": "npx ts-node --project tsconfig.node.json scripts/generate-nodespec-docs.ts",
		"generate:command-docs": "npx ts-node --project tsconfig.node.json scripts/generate-command-docs.ts",
		"check:token-drift": "git diff --exit-code app/styles/_generated_tokens.css",
		"validate:tokens": "node scripts/theming/validate-tokens.js",
		"ci:tokens": "pnpm validate:tokens",
		"validate:colors": "node scripts/theming/validate-primitive-colors.js",
		"validate:adapter": "node scripts/validate-adapter-logic.js",
		"version:analyze": "npx ts-node --project tsconfig.node.json scripts/version-from-commits.ts",
		"version:bump": "pnpm version:analyze",
		"commitlint": "commitlint --edit",
		"postinstall": "pnpm generate:docs",
		"sentry:sourcemaps": "sentry-cli sourcemaps inject --org utilitek-solutions --project agenitix .next && sentry-cli sourcemaps upload --org utilitek-solutions --project agenitix .next",
		"sentry:sourcemaps:conditional": "node -e \"if (process.env.SENTRY_AUTH_TOKEN && process.env.VERCEL_ENV === 'production') { require('child_process').execSync('sentry-cli sourcemaps inject --org utilitek-solutions --project agenitix .next && sentry-cli sourcemaps upload --org utilitek-solutions --project agenitix .next', { stdio: 'inherit', shell: true }); } else { console.log('Skipping Sentry sourcemap upload - not in production or missing SENTRY_AUTH_TOKEN'); }\""
	},
	"dependencies": {
		"@ai-sdk/anthropic": "^1.2.12",
		"@ai-sdk/google": "^1.2.22",
		"@ai-sdk/openai": "^1.3.23",
